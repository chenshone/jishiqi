.container {
	margin: 40rpx 50rpx 0 50rpx;
}

.eventText {
	display: flex;
}

.event {
	display: flex;
	flex-direction: column;
}

.t-event {
	color: #000000;
	font-size: 30rpx;
	height: 52rpx;
	width: 125rpx;
	display: flex;
	justify-content: space-between;
}

.i-event {
	width: 125rpx;
	height: 10rpx;
}

.event-xiang {
	color: #AAAAAA;
	font-size: 15px;
	width: 100%;
	height: 50rpx;
	border-bottom: 1px solid #AAAAAA;
	margin-left: 10rpx;
}

.Time {
	display: flex;
	margin-top: 50rpx;
}

.Jiantou {
	display: flex;
	justify-content: space-between;
	width: 100%;
	margin-left: 130rpx;
	margin-top: 20rpx;
	width: 60rpx;
}

.start-box,
.end-box {
	width: 20rpx;
	height: 60rpx;
	display: flex;
	align-items: center;
}

.start-box-border {
	border-right: 1px solid #000000;
	border-bottom: 1px solid #000000;
}

.end-box-border {
	border-left: 1px solid #000000;
	border-top: 1px solid #000000;
}

.start-circle,.end-circle {
	color: #AAAAAA;
	border: 1px solid #000000;
	border-radius: 50%;
	font-size: 20rpx;
	display: flex;
	align-items: center;
	justify-content: center;
	width: 34rpx;
	height: 34rpx;
}

.start-circle-border{
	margin-left: -20rpx;
	margin-bottom: -2rpx;
}

.end-circle-border {
	margin-right: -20rpx;
	margin-top: -2rpx;
}

.time {
	display: flex;
	flex-direction: column;
}

.time-content {
	width: 100%;
}

.t-time {
	color: #ff3434;
	font-size: 15px;
	height: 51rpx;
	width: 125rpx;
	display: flex;
	justify-content: space-between;
}

.i-time {
	width: 125rpx;
	height: 10rpx;
}

.time_qi {
	font-size: 30rpx;
	color: #A2A2A2;
	height: 50rpx;
	margin-left: 10rpx;
}

.time_zhong {
	padding-top: 14rpx;
	font-size: 30rpx;
	color: #A2A2A2;
	margin-left: 10rpx;
	height: 50rpx;
	border-bottom: 1px solid #AAAAAA;
}

.xiang {
	display: flex;
	margin-top: 50rpx;
}

.neirong {
	display: flex;
	flex-direction: column;
}

.t-neirong {
	font-size: 15px;
	color: #000000;
	display: flex;
	justify-content: space-between;
}

.i-neirong {
	width: 125rpx;
	height: 10rpx;
}



.content-detail {
	font-size: 15px;
	color: #A2A2A2;
	width: 486.4rpx;
	border-bottom: 1px solid #AAAAAA;
	margin-left: 10rpx;
	word-break: break-all;
}

.start_Time {
	display: flex;
	margin-top: 50rpx;
}

.stat_time {
	font-size: 15px;
	color: #A2A2A2;
	height: 50rpx;
	width: 495rpx;
	border-bottom: 1px solid #AAAAAA;
}

.start {
	display: flex;
	flex-direction: column;
}

.end_Time {
	display: flex;
	margin-top: 50rpx;
}

.end {
	display: flex;
	flex-direction: column;
}

.end_time {
	font-size: 15px;
	color: #A2A2A2;
	height: 50rpx;
	width: 495rpx;
	border-bottom: 1px solid #AAAAAA;
}

.Beizhu {
	margin-top: 50rpx;
	display: flex;
}

.beizhu {
	display: flex;
	flex-direction: column;
}

.beizhu_xiang {
	font-size: 15px;
	color: #A2A2A2;
	width: 540rpx;
	height: 100rpx;
}

.t {
	font-size: 15px;
	color: #000000;
	height: 50rpx;
}

.i {
	width: 135rpx;
	height: 10rpx;
}

.event,
.time,
.neirong,
.start,
.end,
.beizhu {
	margin-right: 30rpx;
}
