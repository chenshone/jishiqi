.container {
	margin: 40rpx 50rpx 0 50rpx;
}

.eventText {
	display: flex;
}

.event {
	display: flex;
	flex-direction: column;
}

.t-event {
	color: #000000;
	font-size: 30rpx;
	height: 52rpx;
	width: 125rpx;
	display: flex;
	justify-content: space-between;
}

.i-event {
	width: 125rpx;
	height: 10rpx;
}

.event-xiang {
	font-size: 15px;
	width: 100%;
	height: 50rpx;
	border-bottom: 1px solid #AAAAAA;
	margin-left: 10rpx;
}

.Time {
	display: flex;
	margin-top: 50rpx;
}

.Jiantou {
	display: flex;
	justify-content: space-between;
	width: 100%;
	margin-left: 130rpx;
	margin-top: 20rpx;
	width: 60rpx;
}

.start-box,
.end-box {
	width: 20rpx;
	height: 60rpx;
	display: flex;
	align-items: center;
}

.start-box-border {
	border-right: 1px solid #000000;
	border-bottom: 1px solid #000000;
}

.end-box-border {
	border-left: 1px solid #000000;
	border-top: 1px solid #000000;
}

.start-circle,.end-circle {
	border: 1px solid #000000;
	border-radius: 50%;
	font-size: 20rpx;
	display: flex;
	align-items: center;
	justify-content: center;
	width: 34rpx;
	height: 34rpx;
}

.start-circle-border{
	margin-left: -20rpx;
	margin-bottom: -2rpx;
}

.end-circle-border {
	margin-right: -20rpx;
	margin-top: -2rpx;
}

.time {
	display: flex;
	flex-direction: column;
}

.time-content {
	width: 100%;
}

.t-time {
	color: #ff3434;
	font-size: 15px;
	height: 51rpx;
	width: 125rpx;
	display: flex;
	justify-content: space-between;
}

.i-time {
	width: 125rpx;
	height: 10rpx;
}

.time_qi {
	font-size: 30rpx;
	height: 50rpx;
	margin-left: 10rpx;
}

.time_zhong {
	padding-top: 14rpx;
	font-size: 30rpx;
	margin-left: 10rpx;
	height: 50rpx;
	border-bottom: 1px solid #AAAAAA;
}

.xiang {
	display: flex;
	margin-top: 50rpx;
}

.neirong {
	display: flex;
	flex-direction: column;
}

.t-neirong {
	font-size: 15px;
	color: #000000;
	display: flex;
	justify-content: space-between;
}

.i-neirong {
	width: 125rpx;
	height: 10rpx;
}



.content-detail {
	font-size: 15px;
	width: 486.4rpx;
	border-bottom: 1px solid #AAAAAA;
	margin-left: 10rpx;
	word-break: break-all;
}

.start_Time,.end_Time,.Beizhu {
	display: flex;
	margin-top: 50rpx;
}

.stat_time,.end_time  {
	font-size: 15px;
	height: 50rpx;
	width: 495rpx;
	border-bottom: 1px solid #AAAAAA;
}

.start,.end,.beizhu {
	display: flex;
	flex-direction: column;
}


.beizhu_xiang {
	font-size: 15px;
	width: 540rpx;
}

.t {
	font-size: 15px;
	height: 50rpx;
}

.i {
	width: 135rpx;
	height: 10rpx;
}

.event,
.time,
.neirong,
.start,
.end,
.beizhu {
	margin-right: 30rpx;
}

.footer{
	position: absolute;
	width: 100%;
	bottom: 70rpx;
	color: #d81e06;
}

.footer-view{
	display: flex;
	flex-flow: row-reverse;
	margin-right: 100rpx;
	justify-content: space-between;
}

.btn-del,.btn-ok,.box{
	display: flex;
	align-items: center;
	font-size: 28rpx;
}

.box{
	width: 156rpx;
}

.btn-finished{
	font-size: 40rpx;
	border: 4rpx solid #d81e06;
	border-radius: 40rpx;
	width: 250rpx;
	height: 70rpx;
	display: flex;
	align-items: center;
	justify-content: center;
}

.box-del{
	border-bottom: 3.5rpx solid #d81e06;
	display: flex;
	align-content: center;
	padding-bottom: 4rpx;
}

.box-del text{
	padding-top: 4rpx;
}

.img-del,.img-ok{
	width: 44rpx;
	height: 44rpx;
}

.line{
	width: 2rpx;
	height: 108rpx;
	background-color: #d81e06;
}

.isok{
	display: flex;
	flex-flow: row-reverse;
	width: 330rpx;
	justify-content: space-between;
}

.box-center{
	justify-content: center;
}

.txt-finished{
	padding-right: 4rpx;
}